{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+alt+t"
        },
        {
            "command": "scrollUpPage",
            "keys": "ctrl+shift+space"
        },
        {
            "command": "closePane",
            "keys": "ctrl+alt+x"
        },
        {
            "command": "scrollDownPage",
            "keys": "ctrl+space"
        },
        {
            "command": 
            {
                "action": "scrollUp"
            },
            "keys": "ctrl+up"
        },
        {
            "command": 
            {
                "action": "scrollDown"
            },
            "keys": "ctrl+down"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "keys": "ctrl+tab"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "keys": "ctrl+shift+tab"
        },
        {
            "command": "closeTab",
            "keys": "ctrl+w"
        },
        {
            "command": "closeWindow",
            "keys": "ctrl+shift+w"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
